    <script id="indexView" type="text/x-handlebars-template">
      <div class="jumbotron">
        <h1 class="text-center">Welcome To Datsy!</h1>
        <p class="text-center lead">We set data free.</p>
        <button id="visualize-data" class="btn-danger btn-lg col-md-offset-3">Visualize Data</button>
        <button id="explore-data" class="btn-primary btn-lg col-md-offset-2">Explore Datasets</button>
      </div>
    </script>
    <script id="visualizations" type="text/x-handlebars-template">
      <h1 class="text-center">Data Comes Alive!</h1>
      <form role="form" class="form-inline center">
        <div class="form-group col-md-6">
          <label for="inputData1" class="sr-only">Data Set One</label>
          <input id="inputData1" type="text" placeholder="Data Set One" class="form-control">
          <button id="inputButton1" type="submit" class="btn btn-default form-control">Add</button>
        </div>
        <div class="form-group col-md-6">
          <label for="inputData2" class="sr-only">Data Set Two</label>
          <input id="inputData2" type="text" placeholder="Data Set Two" class="form-control">
          <button id="inputButton2" type="submit" class="btn btn-default form-control">Add</button>
        </div>
      </form>
      <div id="dataColumnOptions" class="row">
        <div id="dataSetsOne" class="col-md-6"></div>
        <div id="dataSetsTwo" class="col-md-6"></div>
      </div>
      <div id="graph"></div>
    </script>
    <script id="dropper" type="text/x-handlebars-template">
      <div id="dropper" class="col-md-7 col-md-offset-2">
        <ul class="row">
          <li class="xAxis droppable col-md-5">
            <p>Drop X axis value here</p>
          </li>
          <li class="yAxis droppable col-md-5 col-md-offset-2">
            <p>Drop Y axis value here</p>
          </li>
        </ul>
        <button id="renderChart" disabled class="btn btn-success btn-lg center">Render</button>
      </div>
    </script>
    <script id="columnModel" type="text/x-handlebars-template">
      <p>{{ columnTitle }}</p>
    </script>
    <script id="exploreData" type="text/x-handlebars-template">
      <h1 class="text-center">Explore Datasets</h1>
